<?xml version="1.0" encoding="UTF-8"?>
<ContentPage Title="Current Events" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TechFest.Pages.CurrentEventsPage">
	<ContentPage.Content>
		<ListView ItemsSource="{Binding Events}" SelectedItem="{Binding SelectedEvent}" SeparatorVisibility="None">
    		<ListView.ItemTemplate>
      			<DataTemplate>
        			<TextCell Text="{Binding Title}" />
      			</DataTemplate>
    		</ListView.ItemTemplate>
  		</ListView>
	</ContentPage.Content>
</ContentPage>